<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ws="http://jax-ws.dev.java.net/spring/core"
	xmlns:wss="http://jax-ws.dev.java.net/spring/servlet"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd 
	 http://jax-ws.dev.java.net/spring/core 
	 http://jax-ws.dev.java.net/spring/core.xsd 
	 http://jax-ws.dev.java.net/spring/servlet 
	 http://jax-ws.dev.java.net/spring/servlet.xsd">

<!-- 
	<wss:binding url="/PlantSOAPService" service="#plantService" /> 
	<ws:service id="plantService" impl="com.rentit.soap.web.PlantSOAPService" />  -->


 	<wss:binding url="/PlantSOAPService">
		<wss:service>
			<ws:service bean="#plantService" />
		</wss:service>
	</wss:binding>
	<bean id="plantService" class="com.rentit.soap.web.PlantSOAPService" /> 
	
	<wss:binding url="/PurchaseOrderService">
		<wss:service>
			<ws:service bean="#po" />
		</wss:service>
	</wss:binding>
	<bean id="po" class="com.rentit.soap.web.PurchaseOrderService" /> 
	
	
</beans> 
